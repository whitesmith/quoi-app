{
  "name": "wsq-app",
  "version": "0.0.0",
  "description": "A mobile quiz app inspired by Buzz and Dr. Why.",
  "main": "index.js",
  "scripts": {
    "babel-node": "babel-node --presets=es2015",
    "clean": "rm -rf build && mkdir build",
    "build-server-src": "node_modules/.bin/babel src/server -d build/server",
    "build-server": "npm run clean && npm run build-server-src",
    "start-server-dev": "node_modules/.bin/nodemon --exec npm run babel-node -- src/server/index.js",
    "start-server": "node ./build/server/index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/whitesmith/wsq-app.git"
  },
  "keywords": [
    "quiz"
  ],
  "author": "Whitesmith",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/whitesmith/wsq-app/issues"
  },
  "homepage": "https://github.com/whitesmith/wsq-app#readme",
  "dependencies": {
    "babel-cli": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "es6-enum": "^1.0.3",
    "es6-error": "^2.1.0",
    "socket.io": "^1.4.5"
  },
  "devDependencies": {
    "nodemon": "^1.9.1"
  }
}
